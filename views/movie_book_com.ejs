    <style type="text/css" data-asas-style="">
        body,
        div,
        a,
        p,
        span {
            user-select: text !important;
        }

        p,
        h1,
        h2,
        h3,
        h4,
        h5,
        h6 {
            cursor: auto;
            user-select: text !important;
        }

        ::selection {
            background-color: #338FFF !important;
            color: #fff !important;
        }


        #log {
            background-image: -webkit-linear-gradient(169deg, #5560ff 17%, #aa52a1 63%, #ff4343 100%);
            -webkit-transition: all ease 0.3s;
            transition: all ease 0.3s;
            border-radius: 30px;
            width: auto;
            /* padding-top: 20px; */
            padding: 0 50px;
            height: 50px;
            text-transform: uppercase;
            margin: 0 auto;
            font-weight: bold;
            color: white;
        }

        .wrapper {
            background: #338FFF;
            width: 580px;
            border-radius: 15px;
            padding: 20px;
            box-shadow: 10px 10px 15px rgba(0, 0, 0, 0.4);
            margin-left: 32%;
        }

        h2 {
            text-align: center;
        }

        hr {
            width: 100px;
            margin: 10px auto;
        }

        .members {
            display: flex;
            margin: auto;
        }

        .team-mem {
            margin: auto;
        }

        #op {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            margin: 10px;
        }

        h4,
        p {
            text-align: center;
            font-size: 12px;
            margin: 7px;
        }

        .section-404 .custom-button {
            padding: 16px 45px;
            border-radius: 30px;
        }
    </style>
